{% macro displayRingContent(name1, score1, name2, score2, ringName) %}
   <div class="row ring-header">
        <div class="col-md-2 ring-logo"></div>  
        <div class="col-md-6 ring-name">{{ringName}}</div>
        <div class="col-md-4 ring-value">{{min(score1,score2)}}</div>  
    </div>
    <div class="row ring-attribut">
        <div class="col-md-8 attribut-name">{{name1}}</div>
        <div class="col-md-4 attribut-value">{{score1}}</div>  
    </div>    
    <div class="row ring-attribut">
        <div class="col-md-8 attribut-name">{{name2}}</div>
        <div class="col-md-4 attribut-value">{{score2}}</div>  
    </div>
{% endmacro %}
{% macro displayVoidRingContent(score, currentPoints) %}
    <div class="row ring-header">
        <div class="col-md-2 ring-logo"></div>  
        <div class="col-md-6 ring-name">Void</div>
        <div class="col-md-3 ring-value">{{score}}</div>  
    </div>
    <div class="row ring-attribut">
        <div class="col-md-12">
            <div class="row void-point-label">
                <div class="col-md-12">
                    Void Points
                </div>
            </div>
            <div class="row void-point-bar">
                <div class="col-md-12">
                    {% if currentPoints is defined %}
                        {% for i in range(0,9) %}
                            {% if currentPoints > i %}
                                <span class="full-dot"></span>
                            {% else %}  
                                {% if score > i %}
                                    <span class="empty-dot"></span>
                                {% else %}  
                                    <span class="temporary-dot"></span>
                                {% endif %}   
                            {% endif %}                 
                        {% endfor %}                 
                    {% endif %} 
                </div>
            </div>
        </div>
    </div>
{% endmacro %}
{% macro displaySocialScore(score, name) %}
   <div class="col-md-12">
        <div class="row">
            <div class="col-md-6 social-score-label">{{name}}</div>
            <div class="col-md-5 social-score-rank">{{ score // 10 }}</div>    
        </div>
        <div class="row">
            {% set unityScore = score % 10 %}
            {% if unityScore is defined %}
                <span class="spacer col-md-1"></span>
                {% for i in range(0,9) %}
                    {% if unityScore > i %}
                        <span class="social-score-full-dot"></span>
                    {% else %}  
                        <span class="social-score-empty-dot"></span>
                    {% endif %}                 
                {% endfor %}    
                <span class="spacer col-md-1"></span>             
            {% endif %}   
        </div>
   </div>
{% endmacro %}
{% macro displaySkillTable (stringtable) %}
    {% if stringtable is defined %}
        {% set cpt = 1 %}
        {% set stringTab = stringtable|trim |split(';') %} 
        {% for string in stringTab %}
                {% if string is defined and string|length > 0 %}
                    {% if cpt is even %}
                    <div class="row skills-row-even">
                    {% else %}
                    <div class="row skills-row-odd">
                    {% endif %}
                        {% set valueTab = string|trim |split('|') %} 
                        <div class="col-md-3 skill-name">{{ valueTab[0] }}</div>
                        {% set score = valueTab[1]|number_format %}
                        <div class="col-md-1 skill-score">{{score}}</div>
                        <div class="col-md-1 skill-associated-attr">{{ valueTab[2] }}</div>
                        <div class="col-md-7 skill-special">{{  valueTab[3] }}</div>    
                    </div>                                  
                {% endif %}
            {% set cpt = cpt + 1 %}
        {% endfor %}
    {% endif %}   
{% endmacro %}
{% macro displayWeaponTable (stringtableWeapon) %}
    {% if stringtableWeapon is defined %}
        {% set cpt = 1 %}
        {% set stringTabWeapon = stringtableWeapon|trim |split(';') %} 
        {% for stringWeapon in stringTabWeapon %}
            {% if stringWeapon is defined and stringWeapon|length > 0 %}
                <div class="col-md-4 Weapon-block">
                    <div class="row weapon-block-title">
                        Weapon
                    </div>
                    <div class="row weapon-block-data">
                        <div class="col-md-12">
                         {% set valueTab = stringWeapon|trim |split('|') %} 
                            <div class="row">
                                <div class="col-md-6 weapon-block-data-label">Type</div>
                                <div class="col-md-6 weapon-block-data-value">{{ valueTab[0] }}</div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 weapon-block-data-label">Attack Roll</div>
                                <div class="col-md-6 weapon-block-data-value">{{ valueTab[1] }}</div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 weapon-block-data-label">Damage Roll</div>
                                <div class="col-md-6 weapon-block-data-value">{{ valueTab[2] }}</div>
                            </div>
                            <div class="row">
                                <div class="col-md-6 weapon-block-data-label">Bonus</div>  
                                <div class="col-md-6 weapon-block-data-value">{{ valueTab[3] }}</div>                               
                            </div>
                            <div class="row">                             
                                <div class="col-md-6 weapon-block-data-label">Notes</div> 
                                <div class="col-md-6 weapon-block-data-value">{{ valueTab[4] }}</div>                               
                            </div>                     
                        </div>
                    </div>
                </div>                                   
            {% endif %}
            {% set cpt = cpt + 1 %}
        {% endfor %}
    {% endif %}   
{% endmacro %}
