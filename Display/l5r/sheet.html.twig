            <h2 class="row">Wounds</h2>
            <div class="row">
                <div class="col-md-12 woundsTable">
                {% set earth = min(variables.stamina,variables.willpower) %}
                    <div class="row small-table-row">
                        <div class="col-md-6 small-table-header">
                            Wound Level
                        </div>
                        <div class="col-md-2 small-table-header">    
                            Pena.
                        </div>
                        <div class="col-md-2 small-table-header">    
                            Total
                        </div>
                        <div class="col-md-2 small-table-header">    
                            Current
                        </div>
                    </div>
                    {% set healthy_wounds = earth * 5 %}
                    {% set global_current_wounds = variables.global_current_wounds %}
                    {% set global_wounds_penality  = variables.global_wounds_penality %}
                    {%  if ( global_wounds_penality  >= healthy_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - healthy_wounds %}
                        {% set healthy_wounds_penality = healthy_wounds %}
                    {% else %}
                        {% set healthy_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set healthy_total  = healthy_wounds - healthy_wounds_penality %}
                    {%  if ( global_current_wounds  >= healthy_total ) %}       
                        {% set global_current_wounds  = global_current_wounds  - healthy_total %}
                        {% set healthy_current_wounds = healthy_total %}
                    {% else %}
                        {% set healthy_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if healthy_current_wounds == healthy_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if healthy_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Healthy (+0)
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ healthy_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ healthy_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ healthy_current_wounds }}
                        </div>
                    </div>
                    {% set nicked_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= nicked_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - nicked_wounds %}
                        {% set nicked_wounds_penality = nicked_wounds %}
                    {% else %}
                        {% set nicked_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set nicked_total  = nicked_wounds - nicked_wounds_penality %}
                    {%  if ( global_current_wounds  >= nicked_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - nicked_total %}
                        {% set nicked_current_wounds = nicked_total %}
                    {% else %}
                        {% set nicked_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if nicked_current_wounds == nicked_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if nicked_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Nicked (+3)
                        </div>
                        <div class="col-md-2 small-table-value">
                            {{ nicked_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ nicked_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ nicked_current_wounds }}
                        </div>
                    </div>
                    {% set grazed_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= grazed_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - grazed_wounds %}
                        {% set grazed_wounds_penality = grazed_wounds %}
                    {% else %}
                        {% set grazed_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set grazed_total  = grazed_wounds - grazed_wounds_penality %}
                    {%  if ( global_current_wounds  >= grazed_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - grazed_total %}
                        {% set grazed_current_wounds = grazed_total %}
                    {% else %}
                        {% set grazed_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if grazed_current_wounds == grazed_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if grazed_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Grazed (+5)
                        </div>
                        <div class="col-md-2 small-table-value">   
                            {{ grazed_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ grazed_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds"> 
                            {{ grazed_current_wounds }}
                        </div>
                    </div>
                    {% set hurt_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= hurt_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - hurt_wounds %}
                        {% set hurt_wounds_penality = hurt_wounds %}
                    {% else %}
                        {% set hurt_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set hurt_total  = hurt_wounds - hurt_wounds_penality %}
                    {%  if ( global_current_wounds  >= hurt_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - hurt_total %}
                        {% set hurt_current_wounds = hurt_total %}
                    {% else %}
                        {% set hurt_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if hurt_current_wounds == hurt_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if hurt_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Hurt (+10)
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ hurt_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ hurt_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ hurt_current_wounds }}
                        </div>
                    </div>
                    {% set injured_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= injured_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - injured_wounds %}
                        {% set injured_wounds_penality = injured_wounds %}
                    {% else %}
                        {% set injured_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set injured_total  = injured_wounds - injured_wounds_penality %}
                    {%  if ( global_current_wounds  >= injured_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - injured_total %}
                        {% set injured_current_wounds = injured_total %}
                    {% else %}
                        {% set injured_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if injured_current_wounds == injured_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if injured_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Injured (+15)
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ injured_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ injured_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ injured_current_wounds }}
                        </div>
                    </div>
                    {% set crippled_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= crippled_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - crippled_wounds %}
                        {% set crippled_wounds_penality = crippled_wounds %}
                    {% else %}
                        {% set crippled_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set crippled_total  = crippled_wounds - crippled_wounds_penality %}
                    {%  if ( global_current_wounds  >= crippled_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - crippled_total %}
                        {% set crippled_current_wounds = crippled_total %}
                    {% else %}
                        {% set crippled_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if crippled_current_wounds == crippled_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if crippled_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                       <div class="col-md-6 small-table-label">
                            Crippled (+20)
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ crippled_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ crippled_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ crippled_current_wounds }}
                        </div>
                    </div>
                    {% set down_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= down_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - down_wounds %}
                        {% set down_wounds_penality = down_wounds %}
                    {% else %}
                        {% set down_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set down_total  = down_wounds - down_wounds_penality %}
                    {%  if ( global_current_wounds  >= down_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - down_total %}
                        {% set down_current_wounds = down_total %}
                    {% else %}
                        {% set down_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if down_current_wounds == down_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if down_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Down (+40)
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ down_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ down_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ down_current_wounds }}
                        </div>
                    </div>
                    {% set out_wounds = earth * 2 %}
                    {%  if ( global_wounds_penality  >= out_wounds ) %}
                        {% set global_wounds_penality  = global_wounds_penality  - out_wounds %}
                        {% set out_wounds_penality = out_wounds %}
                    {% else %}
                        {% set out_wounds_penality = global_wounds_penality %}
                        {% set global_wounds_penality = 0 %}
                    {% endif %}
                    {% set out_total  = out_wounds - out_wounds_penality %}
                    {%  if ( global_current_wounds  >= out_total ) %}
                        {% set global_current_wounds  = global_current_wounds  - out_total %}
                        {% set out_current_wounds = out_total %}
                    {% else %}
                        {% set out_current_wounds = global_current_wounds %}
                        {% set global_current_wounds = 0 %}
                    {% endif %}
                    {% if out_current_wounds == out_total %}
                        <div class="row small-table-row wound-empty">
                    {% else %}
                        {% if out_current_wounds > 0 %}
                            <div class="row small-table-row wound-damaged">
                        {% else %}
                            <div class="row small-table-row wound-full">
                        {% endif %}
                    {% endif %}
                        <div class="col-md-6 small-table-label">
                            Out
                        </div>
                        <div class="col-md-2 small-table-value"> 
                            {{ out_wounds_penality }}
                        </div>
                        <div class="col-md-2 small-table-value">    
                            {{ out_total }}
                        </div>
                        <div class="col-md-2 small-table-value current-wounds">    
                            {{ out_current_wounds }}
                        </div>
                    </div>
                </div>
            </div>
            <h2 class="row">Rate of Wounds Heal</h2>
            <div class="row rohTable-line">
                <div class="col-md-8 small-table-label">Sta. * 2 + Ins. Rank</div>   
                {% set roh = variables.stamina|number_format %}    
                {% set roh = roh * 2 %}    
                {% set roh = roh + variables.rank|number_format %}  
                <div class="col-md-4 small-table-value">{{ roh }}</div>
            </div>
            {% if variables.healt_regen_modifier is defined and variables.healt_regen_modifier|length >0 %}
            <div class="row rohTable-line">
                <div class="col-md-8 small-table-label">Modifier</div>
                <div class="col-md-4 small-table-value">{{ variables.healt_regen_modifier }}</div>
            </div>
            <div class="row rohTable-line">
                <div class="col-md-8 small-table-label">Current</div> 
                {% set rohCurrent = roh + variables.healt_regen_modifier|number_format %}  
                <div class="col-md-4 small-table-value">{{ rohCurrent }}</div>
            </div>
            {% endif %}
        </div> 
    </div>  
